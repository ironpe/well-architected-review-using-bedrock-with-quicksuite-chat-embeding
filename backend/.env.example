# AWS Configuration
AWS_REGION=us-east-1

# DynamoDB Tables
REVIEW_REQUESTS_TABLE=ArchitectureReview-ReviewRequests
DOCUMENTS_TABLE=ArchitectureReview-Documents
REVIEW_EXECUTIONS_TABLE=ArchitectureReview-ReviewExecutions
PILLAR_CONFIGURATIONS_TABLE=ArchitectureReview-PillarConfigurations
GOVERNANCE_POLICIES_TABLE=ArchitectureReview-GovernancePolicies

# S3 Buckets
DOCUMENTS_BUCKET=architecture-review-documents
REPORTS_BUCKET=architecture-review-reports

# Amazon Bedrock
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0
BEDROCK_AGENT_TIMEOUT=300000

# AWS Q Business
Q_BUSINESS_APPLICATION_ID=your-q-business-app-id
Q_BUSINESS_DATA_SOURCE_ID=your-data-source-id

# Amazon SES
SES_FROM_EMAIL=noreply@example.com
SES_REPLY_TO_EMAIL=support@example.com

# Cognito
COGNITO_USER_POOL_ID=your-user-pool-id
COGNITO_CLIENT_ID=your-client-id

# QuickSight Chat Agent Embedding
QUICKSIGHT_ACCOUNT_ID=your-aws-account-id
QUICKSIGHT_AGENT_ARN=arn:aws:quicksight:us-east-1:YOUR_ACCOUNT_ID:agent/YOUR_AGENT_ID
QUICKSIGHT_NAMESPACE=default
QUICKSIGHT_USER_NAME=your-quicksight-user
QUICKSIGHT_EMBED_URL=https://us-east-1.quicksight.aws.amazon.com/sn/embed/share/accounts/YOUR_ACCOUNT_ID/chatagents/YOUR_AGENT_ID?directory_alias=YOUR_ACCOUNT_NAME
